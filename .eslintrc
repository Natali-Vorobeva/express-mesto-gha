{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "eol-last": [
      "error",
      "never"
    ],
    "padded-blocks": 0,
    "semi": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "object-curly-newline": 0,
    "no-useless-return": 0
  }
}